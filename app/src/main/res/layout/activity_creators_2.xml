<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_background_color">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="20sp"
                android:background="@drawable/background"
                android:padding="10sp"
                android:text="Below are the icons and their authors.\nFor more, click on the icon"
                android:textAlignment="center"
                android:textColor="@color/app_text_color"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:text="judanna"
                android:textColor="@color/app_text_color"
                android:textSize="16sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ID_Judanna_Fire"
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@drawable/add_data_dark_background_for_activities"
                        android:padding="10sp"
                        app:srcCompat="@drawable/fire" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:text="Pixel perfect"
                android:textColor="@color/app_text_color"
                android:textSize="16sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ID_PixelPerfect_UserSettings"
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@drawable/add_data_dark_background_for_activities"
                        android:padding="10sp"
                        app:srcCompat="@drawable/profile_view_settings" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:text="Us and Up "
                android:textColor="@color/app_text_color"
                android:textSize="16sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ID_UsAndUp_running"
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@drawable/add_data_dark_background_for_activities"
                        android:padding="10sp"
                        app:srcCompat="@drawable/running_icon" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:text="Prosymbols Premium "
                android:textColor="@color/app_text_color"
                android:textSize="16sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ID_ProsymbolsPremium_sleepingIcon"
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@drawable/add_data_dark_background_for_activities"
                        android:padding="10sp"
                        app:srcCompat="@drawable/sleeping_icon" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:text="juicy_fish  "
                android:textColor="@color/app_text_color"
                android:textSize="16sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ID_juicy_fish_BMI"
                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@drawable/add_data_dark_background_for_activities"
                        android:padding="10sp"
                        app:srcCompat="@drawable/profile_view_bmi" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="80sp"
                        android:layout_margin="10sp"
                        android:layout_weight="1"
                        android:background="@color/app_background_color"
                        android:padding="10sp" />
                </LinearLayout>
            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/ID_Credits2Button"
                    android:layout_width="29dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="160dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageView8"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/page_change_circle_full" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:textAlignment="center"
                    android:textColor="@color/app_background_color"
                    app:layout_constraintBottom_toBottomOf="@+id/ID_Credits2Button"
                    app:layout_constraintEnd_toEndOf="@+id/ID_Credits2Button"
                    app:layout_constraintStart_toStartOf="@+id/ID_Credits2Button"
                    app:layout_constraintTop_toTopOf="@+id/ID_Credits2Button"
                    tools:textStyle="bold" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="30sp"
                    android:layout_height="30sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="160dp"
                    android:layout_marginBottom="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/page_change_circle_empty" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:textAlignment="center"
                    android:textColor="@color/app_text_color"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView8"
                    app:layout_constraintEnd_toEndOf="@+id/imageView8"
                    app:layout_constraintStart_toStartOf="@+id/imageView8"
                    app:layout_constraintTop_toTopOf="@+id/imageView8"
                    tools:textStyle="bold" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>